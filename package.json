{
  "name": "echopilot",
  "version": "1.0.0",
  "description": "A comprehensive review monitoring platform built with React and Supabase",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:infrastructure": "node scripts/run-tests.js",
    "test:database": "node scripts/run-tests.js --suite database",
    "test:api-keys": "node scripts/run-tests.js --suite apiKeys",
    "test:environment": "node scripts/run-tests.js --suite environment",
    "test:performance": "node scripts/run-tests.js --suite performance",
    "test:error-handling": "node scripts/run-tests.js --suite errorHandling",
    "test:integration": "node scripts/run-tests.js --mode integration",
    "test:all": "node scripts/run-tests.js --suite all --mode all",
    "test:cleanup": "node scripts/cleanup-test-data.js",
    "test:cleanup-verify": "node scripts/cleanup-test-data.js --verify",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "tsx scripts/seed.ts",
    "functions:deploy": "supabase functions deploy",
    "functions:new": "supabase functions new",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/react-query": "^5.84.1",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.536.0",
    "next": "^14.2.31",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.5.2",
    "react-intersection-observer": "^9.16.0",
    "react-query": "^3.39.3",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "recharts": "^2.15.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/analytics": "^1.5.0",
    "dotenv": "^16.6.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.31",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "supabase": "^1.226.4",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
